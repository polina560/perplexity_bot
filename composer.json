{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "laravel/framework": "^12.39",
        "laravel/octane": "^2.13",
        "laravel/sanctum": "^4.2",
        "laravel/tinker": "^2.10",
        "league/flysystem-aws-s3-v3": "^3.30",
        "longman/telegram-bot": "*",
        "moonshine/apexcharts": "^2.0",
        "moonshine/changelog": "^3.0",
        "moonshine/ckeditor": "^3.0",
        "moonshine/import-export": "^2.0",
        "moonshine/moonshine": "^4.0",
        "moonshine/permissions": "^3.0",
        "moonshine/ru": "^1.0",
        "moonshine/two-factor": "^3.0",
        "openspout/openspout": "^4.28",
        "rap2hpoutre/fast-excel": "^5.6",
        "spatie/laravel-backup": "^9.3",
        "spatie/laravel-medialibrary": "^11.17",
        "symfony/http-foundation": "^7.3",
        "visual-ideas/moonshine-spatie-medialibrary": "^4.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.16",
        "dev-lnk/moonshine-builder": "^3.0",
        "fakerphp/faker": "^1.24",
        "larastan/larastan": "^3.8",
        "laravel/pail": "^1.2",
        "laravel/pint": "^1.25",
        "laravel/sail": "^1.48",
        "laravel/telescope": "^5.15",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "phpunit/phpunit": "^12.2",
        "rector/rector": "^2.0",
        "roave/security-advisories": "dev-latest",
        "zircote/swagger-php": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"npm run dev\" --names='server,queue,vite'"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
